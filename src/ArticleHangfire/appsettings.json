{
  "HttpJobs": {
    "DefaultTimeout": "00:00:05",
    "Endpoints": {
      "Core": {
        "IsAlive": {
          "Cron": "*/5 * * * *",
          "Enabled": true,
          "Address": "https://localhost:7076/api/is-alive",
          "Method": "GET",
          "Headers": {
            "accept": "application/json"
          },
          "Timeout": null,
          "IgnoreInvalidStatusCode": false
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
